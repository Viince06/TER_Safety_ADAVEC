SCADE=D:\Programs Installed\ANSYS Inc\v211\SCADE Student\SCADE\.

SHELL=cmd.exe

C_FLAGS = -pedantic

all: Operator3.exe Operator3.dll 

win64/Operator3.o: Operator3.c
	gcc -I"$(SCADE)\libraries\SC65\libmathext" -I"." -I"$(SCADE)" -I"$(SCADE)\include" -I"$(SCADE)\include\C" -I"$(SCADE)\include\Ada" -I"$(SCADE)\lib\Ada" $(C_FLAGS) -DBUILD_DLL -DSIM -DWIN32 -D_CONSOLE -D_MBCS -c -ansi -std=c99 -m64 "Operator3.c" -o "win64\Operator3.o"

